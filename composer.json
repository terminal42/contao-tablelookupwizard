{
    "name": "terminal42/contao-tablelookupwizard",
    "description": "Contao widget to select records from a foreign table. Useful if you have a large set of records.",
    "keywords": ["contao", "widget", "tablelookupwizard"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "https://www.terminal42.ch"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/terminal42"
        },
        {
            "type": "other",
            "url": "https://ko-fi.com/terminal42"
        }
    ],
    "support": {
        "issues": "https://github.com/terminal42/contao-tablelookupwizard/issues",
        "source": "https://github.com/terminal42/contao-tablelookupwizard"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "^4.9",
        "codefog/contao-haste": "^4.16"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\TableLookupWizardBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\TableLookupWizardBundle\\ContaoManager\\Plugin"
    },
    "scripts": {
        "cs-fixer": "tools/ecs/vendor/bin/ecs check src/ --fix --ansi"
    }
}
